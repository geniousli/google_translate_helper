{

  "description": "create a google dictory",
  "manifest_version": 2,
  "name": "google_translate_helper",
  "version": "1.0",
  "homepage_url": "https://",
  "icons": {
    "48": "icons/translate-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://translate.google.cn/*"],
      "js": ["dictionary.js"],
      "css": ["dictionary.css"]
    }
  ],
  "permissions": [
    "<all_urls>",
  ],
  "homepage_url": "https://example.org/my-addon"
}
